<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="1">

	<listeners>
		<listener class-name="TestPackage.Listners"></listener>
	</listeners>


	<parameter name="URL" value="qaclickacdemy.com"></parameter>

	<test thread-count="5" name="Personal Loan">
		<classes>
			<class name="TestPackage.Day1New">
				<methods>
					<include name="FirstTest">
					</include>
				</methods>
			</class>
			<class name="TestPackage.Day2New" />
			<class name="TestPackage.Day4" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="Car Loan">
		<classes>
			<class name="TestPackage.Day3">
				<methods>
					<exclude name="Mobile.*"> </exclude>

				</methods>
			</class>
		</classes>
	</test> <!--
	Test -->

</suite> <!--
Suite -->
